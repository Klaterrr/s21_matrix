        <definition>int s21_sum_matrix</definition>
        <argsstring>(matrix_t *A, matrix_t *B, matrix_t *result)</argsstring>
        <name>s21_sum_matrix</name>
        <param>
          <type><ref refid="s21__matrix_8h_1a6121abdcc0c0f5e9a8c42390d3274fdd" kindref="member">matrix_t</ref> *</type>
